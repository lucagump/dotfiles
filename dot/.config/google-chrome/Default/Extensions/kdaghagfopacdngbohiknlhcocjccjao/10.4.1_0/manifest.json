{
   "app": {
      "background": {
         "persistent": false,
         "scripts": [ "js/eventPage.js" ]
      }
   },
   "author": "Betaflight Squad",
   "default_locale": "en",
   "description": "Crossplatform configuration tool for Betaflight flight control system",
   "icons": {
      "128": "images/bf_icon_128.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "38",
   "name": "Betaflight - Configurator",
   "offline_enabled": true,
   "permissions": [ "https://maps.googleapis.com/*", "https://*.github.com/", "https://*.githubusercontent.com/", "http://*.baseflight.net/", "https://*.amazonaws.com/", "https://*.betaflight.tech/", "https://www.google-analytics.com/", "serial", "usb", "storage", "fileSystem", "fileSystem.write", "fileSystem.retainEntries", "notifications", "alwaysOnTopWindows", {
      "usbDevices": [ {
         "productId": 57105,
         "vendorId": 1155
      } ]
   }, "webview", "unlimitedStorage" ],
   "short_name": "Betaflight",
   "sockets": {
      "tcp": {
         "connect": "*:*"
      }
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "10.4.1",
   "webview": {
      "partitions": [ {
         "accessible_resources": [ "tabs/map.html", "js/tabs/map.js", "/images/icons/cf_icon_position.png" ],
         "name": "map"
      } ]
   }
}
